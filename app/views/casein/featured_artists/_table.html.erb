<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Name1", :name1 %></th>
			<th><%= casein_sort_link "Name2", :name2 %></th>
			<th><%= casein_sort_link "Bio1", :bio1 %></th>
			<th><%= casein_sort_link "Bio2", :bio2 %></th>
			<th><%= casein_sort_link "Photo", :photo %></th>
			<th><%= casein_sort_link "Photo2", :photo2 %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>

	<% featured_artists.each do |featured_artist| %>
		<tr>
			<td><%= casein_table_cell_link featured_artist.name1, casein_featured_artist_path(featured_artist) %></td>
			<td><%= casein_table_cell_link featured_artist.name2, casein_featured_artist_path(featured_artist) %></td>
			<td><%= casein_table_cell_link featured_artist.bio1, casein_featured_artist_path(featured_artist) %></td>
			<td><%= casein_table_cell_link featured_artist.bio2, casein_featured_artist_path(featured_artist) %></td>
			<td><%= casein_table_cell_link featured_artist.photo, casein_featured_artist_path(featured_artist) %></td>
			<td><%= casein_table_cell_link featured_artist.photo2, casein_featured_artist_path(featured_artist) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_featured_artist_path(featured_artist), :method => :delete, :data => { :confirm => "Are you sure you want to delete this featured artist?" }) %>
			</td>
		</tr>
	<% end %>
</table>
